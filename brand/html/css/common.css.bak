/* 하위 브라우저 안내 페이지 */
.ie8 {display:none; background:#fff; position:fixed; left:0; top:0; width:100%; height:100%; z-index:9999; text-align:center; padding-top:15%;}
.ie8 p {margin-top:30px; font-size:16px; line-height:1.5;}
.ie8 a {display:inline-block; padding:0 10px; height:30px; line-height:30px; background:red; font-size:16px; margin-top:30px; background:#b58913; color:#fff; font-weight:700;}

/* 스킵 네비게이션 */
.skip {position:relative; z-index:2000;}
.skip a {display:block; height:1px; width:1px; margin-left:-1px; margin-bottom:-1px; overflow:hidden; font-size:20px; color:#fff; font-weight:bold; background:#b58913; white-space:nowrap; text-align:center;}
.skip a:focus,
.skip a:active {height:auto; width:100%; padding:5px; margin-bottom:10px; position:absolute; left:0; top:0;}

/* header(모바일) */
header.mobile {height:59px; background:#fff; text-align:center; border-bottom:1px solid #e7e7e7; position:fixed; width:100%; z-index:500;}
header.mobile h1 a {display:inline-block; width:114px; height:30px; background:url(../img/logo_main.png) no-repeat; background-size:114px 30px; text-indent:-9999px; margin-top:15px;}
header.mobile .menu {display:inline-block; width:60px; height:60px; background:url(../img/ico/ico_menu_mobile.png) no-repeat center center; background-size:18px 15px; position:absolute; left:0px; top:0px; text-indent:-9999px;}
header.mobile .deemed {display:none; background:#000; opacity:0.7; position:fixed; left:0; top:0; width:100%; height:100%; z-index:990;}
header.mobile nav {position:fixed; left:-260px; top:0; width:260px; height:100%; background:#fff; text-align:left; z-index:1000;}
header.mobile nav .lang {padding:14px 15px; overflow:hidden;}
header.mobile nav .lang li a {display:inline-block; width:calc(50% - 2px); height:29px; line-height:29px; border:1px solid #d2d2d2; float:left; font-size:13px; color:#909090; font-weight:300; text-align:center;}
header.mobile nav .lang li.on a {border:1px solid #b58913; color:#fff; background:#b58913; font-weight:500;}
header.mobile nav .close {display:none; width:60px; height:60px; background:url(../img/ico/ico_menu_close.png) no-repeat; background-size:60px 60px; position:absolute; left:260px; top:0; text-indent:-9999px;}
header.mobile nav .gnb {border-top:1px solid #e7e7e7;} /* GNB 1depth */
header.mobile nav .gnb > li {border-bottom:1px solid #e7e7e7;}
header.mobile nav .gnb > li > a {display:inline-block; width:calc(100% - 20px); height:49px; line-height:49px; color:#333; font-size:16px; font-weight:600; padding-left:20px; background:url(../img/bl/bl_m1_mobile_off.png) no-repeat 222px center; background-size:18px 9px;}
header.mobile nav .gnb > li > a:only-child {background:none;}
header.mobile nav .gnb > li.on {background:#f6f5f4; border-left:5px solid #b58913;}
header.mobile nav .gnb > li.on > a {color:#b58913; padding-left:15px; background:url(../img/bl/bl_m1_mobile_on.png) no-repeat 217px center; background-size:18px 9px;}
header.mobile nav .gnb > li > ul {display:none;} /* GNB 2depth */
header.mobile nav .gnb > li > ul > li > a {display:inline-block; width:calc(100% - 15px); height:39px; line-height:39px; font-size:12px; color:#666; padding-left:20px; font-weight:400; background:url(../img/bl/bl_m2_mobile_off.png) no-repeat 226px center; background-size:10px 10px; border-top:1px solid #e7e7e7;}
header.mobile nav .gnb > li > ul > li > a:only-child {background:none;}
header.mobile nav .gnb > li.on > ul > li > a {padding-left:15px; background-position: 221px center; border-top:1px solid #e7e7e7;}
header.mobile nav .gnb > li > ul > li.on > a {background:url(../img/bl/bl_m2_mobile_on.png) no-repeat 226px center; background-size:10px 10px;}
header.mobile nav .gnb > li.on > ul > li.on > a {background:url(../img/bl/bl_m2_mobile_on.png) no-repeat 221px center; background-size:10px 10px;}
header.mobile nav .gnb > li > ul > li > ul {display:none; border-top:1px solid #e7e7e7;} /* GNB 3depth */
header.mobile nav .gnb > li > ul > li > ul > li > a {display:inline-block; width:calc(100% - 31px); height:39px; line-height:39px; font-size:12px; color:#909090; padding-left:36px; border-bottom:1px solid #e7e7e7; font-weight:300; background:url(../img/bl/bl_dot_22.png) no-repeat 26px center; background-size:2px 2px;}
header.mobile nav .gnb > li.on > ul > li > ul > li > a {padding-left:31px; background:url(../img/bl/bl_dot_22.png) no-repeat 21px center; background-size:2px 2px;}
header.mobile nav .gnb > li > ul > li > ul > li:last-child > a {border-bottom:none;}
header.mobile nav .gnb > li.on > ul > li:last-child > a {border-bottom:none;}

/* header(PC) */
header.pc {position:absolute; left:0; top:0; width:220px; height:100%; min-height:1024px; background:url(../img/bg/bg_gnb.gif) repeat-y; z-index:1000; /*border-right:1px solid #e7e7e7;*/}
header.pc h1 a {display:inline-block; width:220px; height:149px; background:url(../img/logo_main.png) no-repeat 30px center; text-indent:-9999px; background-size:160px 42px;}
header.pc nav > ul > li {position:relative; width:220px;} /* GNB 1depth */
header.pc nav > ul > li > a {display:inline-block; width:190px; height:49px; line-height:49px; font-size:15px; color:#333; font-weight:600; padding-left:30px; border-bottom:1px solid #e7e7e7; }
header.pc nav > ul > li > a:hover,
header.pc nav > ul > li > a:focus,
header.pc nav > ul > li.on > a {background:#323232 url(../img/bl/bl_m1_more.png) no-repeat 194px center; color:#fff;}
header.pc nav > ul > li:first-child > a {border-top:1px solid #e7e7e7;}
header.pc nav > ul > li > a:only-child:hover,
header.pc nav > ul > li > a:only-child:focus {background:#323232;}
header.pc nav > ul > li > ul {display:none; position:absolute; left:220px; top:-1px; width:220px;} /* GNB 2depth */
header.pc nav > ul > li > ul > li > a {display:inline-block; width:190px; height:49px; line-height:49px; border-bottom:1px solid #505050; font-size:15px; color:#909090; font-weight:600; padding-left:30px; font-weight:500; background:url(../img/bl/bl_m2_pc_off.png) no-repeat 195px center;}
header.pc nav > ul > li > ul > li > a:only-child {background:none;} /* 하위 메뉴 없는 경우 */
header.pc nav > ul > li > ul > li > a:hover,
header.pc nav > ul > li > ul > li > a:focus {color:#fff;}
header.pc nav > ul > li > ul > li.on > a {color:#fff; background:#242424 url(../img/bl/bl_m2_pc_on.png) no-repeat 194px center; border-bottom:1px solid #242424;}
header.pc nav > ul > li > ul > li.none > a {border-bottom:1px solid #505050; background-image:none;}
header.pc nav > ul > li > ul > li:first-child {border-top:1px solid #505050;}
header.pc nav > ul > li > ul > li > ul {display:none; background:#242424;} /* GNB 3depth */
header.pc nav > ul > li > ul > li.on > ul {border-bottom:1px solid #505050;}
header.pc nav > ul > li > ul > li > ul > li {padding-left:45px; background:url(../img/bl/bl_dot.png) no-repeat 35px 7px; height:22px;}
header.pc nav > ul > li > ul > li > ul > li > a {display:inline-block; margin-bottom:10px; font-size:14px; color:#909090; width:150px; word-break:keep-all;}
header.pc nav > ul > li > ul > li > ul > li > a:hover,
header.pc nav > ul > li > ul > li > ul > li > a:focus {color:#fff;}
header.pc nav > ul > li > ul > li > ul > li:last-child {padding-bottom:10px;}

/* 컨텐츠 영역 */
section {padding-left:220px; word-break:keep-all;}
section > .tit {height:149px; border-bottom:1px solid #e7e7e7; background:#f9f8f5;}
section > .tit > div {width:1000px; height:70px; margin:0 auto; padding:40px 0;}
section > .tit > div h2 {font-size:40px; font-weight:700; color:#936d07;}
section > .tit > div p {padding-top:15px; font-size:18px; font-weight:300; color:#666;}
section > .cont > div {width:1000px; margin:0 auto; padding:80px 0; position:relative; overflow:hidden;} /* 컨텐츠 레이아웃 */

section > .cont .loca {position:absolute; overflow:hidden; right:0; top:15px;} /* 로케이션, PC에서만 보임 */
section > .cont .loca li {float:left; display:inline-block; font-size:14px; font-weight:300; color:#909090; padding-left:35px;background:url(../img/ico/ico_next_loca.png) no-repeat 15px 3px;}
section > .cont .loca li:first-child {text-indent:-9999px; background:url(../img/ico/ico_home_loca.png) no-repeat; padding-left:0; width:16px;}
section > .cont .loca li:last-child {color:#ac8724; font-weight:500;}

/* footer(PC) */
footer.pc {position:absolute; left:0; top:450px; width:219px; z-index:1000;}
footer.pc .psn {padding:27px 23px 17px 35px;}
footer.pc .psn li a {display:inline-block; width:164px; height:19px; line-height:19px; font-size:12px; color:#666; background:url(../img/bl/bl_dot.png) no-repeat 0 center; padding-left:10px; font-weight:300;}
footer.pc .sns {padding-left:35px; overflow:hidden;}
footer.pc .sns li {float:left;}
footer.pc .sns li a {display:inline-block; width:30px; height:30px; text-indent:-9999px; margin-right:10px;}
footer.pc .sns li:nth-child(1) a {background:url(../img/ico/ico_fb_gnb.png) no-repeat;} /* 페이스북 */
footer.pc .sns li:nth-child(2) a {background:url(../img/ico/ico_pin_gnb.png) no-repeat;} /* 핀터레스트 */
footer.pc .sns li:nth-child(3) a {background:url(../img/ico/ico_ins_gnb.png) no-repeat;} /* 인스타그램 */
footer.pc .lang {padding:20px 29px 40px 30px;}
footer.pc .lang ul {overflow:hidden;}
footer.pc .lang ul li {display:inline; float:left;}
footer.pc .lang ul li a {display:inline-block; width:78px; line-height:28px; height:28px; font-size:12px; color:#666; text-align:center; border:1px solid #d2d2d2; background:#fff;}
footer.pc .lang ul li a:hover,
footer.pc .lang ul li a:focus,
footer.pc .lang ul li a:active,
footer.pc .lang ul li a.on {border:1px solid #b58913; color:#fff; background:#b58913;}
footer.pc .pol {overflow:hidden; padding-left:30px;}
footer.pc .pol li a {font-size:13px; float:left; padding-right:10px; border-right:1px solid #d2d2d2; padding-bottom:2px; font-weight:300; color:#666;}
footer.pc .pol li:last-child a {padding-left:10px; border-right:none;}
footer.pc dl {padding:15px 10px 15px 30px; font-weight:300;}
footer.pc dl dt {display:inline-block; width:13px; height:21px; line-height:21px; color:#909090;}
footer.pc dl dd {display:inline-block; width:calc(100% - 13px); color:#909090;}
footer.pc dl dd a {color:#909090; font-weight:300;}
footer.pc .copyright {padding:20px 10px 20px 30px; color:#909090; line-height:20px;}

/* 로그인, 로그아웃 구분 */
.login_user {display:none;}

/*********************************************************************** 
mobile 
***********************************************************************/
@media only screen and (min-width :0px) and (max-width : 1279px) {

	.mobile{display:block !important;}
	.pc{display:none !important;}

	html,body{overflow-x:hidden;}

}